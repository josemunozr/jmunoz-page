include layout.jade
#home
.video-container
  video(autoplay='', loop='', poster='./videos/poster/Aloha-Mundo.jpg')
    source(src='./videos/mp4/Aloha-Mundo.mp4', typ='video/mp4')
    source(src='./videos/webm/Aloha-Mundo.webm', typ='video/webm')

nav.nav
  .container
    a(href="#home") José Muñoz Ruiz
    ul
      li
        a(href="#home") Home
      li
        a(href="#portafolio") Portafolio
      li
        a(href="#about-me") Sobre mi
      li
        a(href="#contact") Contactame
.container
  .row.full-height.full-width.center-xs.middle-xs
    .col-xs-12
      .box
        .img-container
          img(src="./imgs/avatar-1.jpg" width="200" height="200")
        .title
          h2 Desarrollador Web
        .row.center-xs
          .col-xs-2
            .box.actions-button
              a(href="#contact" class="contact") Contacto
          .col-xs-2
            .box.actions-button
              a(href="#portafolio" class="portafolio") Portafolio
aside.social
  .row.full-height.full-width.end-xs.middle-xs
    .col-xs-12
      .box
        .share-button.twitter
          a(href="https://twitter.com/JoseMunozR_" target="_blank")
            span
              .fa.fa-twitter
        .share-button.facebook
          a(href="https://www.facebook.com/joozepp" target="_blank")
            span
              .fa.fa-facebook
        .share-button.git
          a(href="https://github.com/josemunozr" target="_blank")
            span
              .fa.fa-git

.icon
  a(href="#about-me")
    span
      .fa.fa-chevron-down
